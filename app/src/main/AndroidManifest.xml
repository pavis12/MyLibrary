<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/library"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/library"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyLibrary"
        tools:targetApi="31">
        <activity
            android:name=".Webactivity"
            android:exported="false" />
        <activity
            android:name=".FavBooks"
            android:exported="false"
            android:label="@string/activity_fav_books"/>
        <activity
            android:name=".currentlyReadingbooks"
            android:exported="false"
            android:label="@string/activity_currently_readingbooks"/>
        <activity
            android:name=".wantoread"
            android:exported="false"
            android:label="@string/acitivity_wantoread"/>
        <activity
            android:name=".alredyreadbooks"
            android:exported="false"
            android:label="@string/activity_alreadyreadbooks"/>
        <activity
            android:name=".book_activity"
            android:exported="false"
            android:label="@string/activity_book"/>
        <activity
            android:name=".SeeAllBooks"
            android:exported="false"
            android:label="@string/activity_see_all_books"/>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>